<!DOCTYPE html>
<html>
  <head>
    <title>TechMatch</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <% if flash[:notice] %>
        <p><%= if flash[:notice] %></p>
      <% end %>
      <% if flash[:alert] %>
        <p><%= if flash[:alert] %></p>
      <% end %>
      
    <%= yield %>
  </body>
</html>
